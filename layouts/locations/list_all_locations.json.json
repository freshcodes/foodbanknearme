{{ define "response" }}
  {{- $e := .Pages -}}
  {
    "section" : "locations",
    "state": "{{ .Params.state }}",
    "city": "{{ .Params.city }}",
    "permalink": "{{ .Permalink }}",
    "count" : "{{ len $e }}",
    "items" : [
    {{- range $i, $r := $e }}
    {{ if $i }}, {{ end }}{{- partialCached "location.json" . .Params.state .Params.city .Params.slug }}
    {{ end -}}
    ]
  }
{{ end }}
