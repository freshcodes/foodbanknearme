{{ define "response" }}
  {
    "section" : "locations/{{ .Params.state }}/{{ .Params.city }}",
    "permalink": "{{ .Permalink }}",
    "count" : "{{ len .Data.Pages }}",
    "items" : [
    {{ range $i, $e := .Data.Pages }}
    {{ if $i }}, {{ end }}{{ .Render "item" }}
    {{ end }}
    ]
  }
{{ end }}
