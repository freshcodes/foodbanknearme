{{ define "response" }}
  {{- $e := sort (where .Site.RegularPages "Type" "location") -}}
  {
    "section" : "locations",
    "state": null,
    "city": null,
    "count" : "{{ len $e }}",
    "permalink": "{{ .Permalink }}",
    "items" : [
    {{- range $i, $r := $e -}}
    {{- if $i -}}, {{- end -}}{{- partialCached "location.json" . .Params.state .Params.city .Params.slug -}}
    {{- end -}}
    ]
  }
{{ end }}
